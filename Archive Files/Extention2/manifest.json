{
"manifest_version": 2,
"name": "Image_Plug",
"description" : "Image plugin for sync images",
"short_name": "DV",
"version": "1.0",

"page_action":{
	"default_icon" :"icon.png",
	"default_popup" : "popup.html",
	"default_title": "Image_Bundle"
	
},

"options_page": "options.html",

"permissions":["tabs", "http://*/*", "https://*/*","contextMenus","bookmarks"],

"optional_permissions": [
        "notifications",
        "downloads"
    ],

"web_accessible_resources": [
        "options.html"
    ]



}