{
"name" : "Img_Plug",
"version": "1.0",
"description" : "Image plugin for sync images",



"browser_action":{
	"popup" : "popup.html",
	"default_title": "Hyperlink-text",
	"default_icon" :"icon.jpeg"
},
"permissions":["tabs", "http://*/*", "https://*/*"],



"content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
      "js" : ["script.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],

  "manifest_version": 2
}